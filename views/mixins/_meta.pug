mixin meta(metaData = {})
  meta(property="og:title" content=`${h.siteTitle || metaData.title}`)
  meta(property="og:site_name" content=`${h.siteName || metaData.siteName}`)
  meta(property="og:url" content="")
  meta(property="og:description"
      content=`${h.siteDescription || metaData.siteDescription}`)
  meta(property="og:type" content="website")
  meta(property="og:image" content="")
  meta(property="og:image:width" content="1080")
  meta(property="og:image:height" content="1080")

  meta(name="twitter:card" content="summary")
  meta(name="twitter:site" content="@feriforgacs")
  meta(name="twitter:title" content=`${h.siteTitle || metaData.title}`)
  meta(name="twitter:description"
      content=`${h.siteDescription || metaData.siteDescription}`)
  meta(name="twitter:image" content="")
  meta(name="twitter:image:alt" content=`${h.siteName || metaData.siteName}`)